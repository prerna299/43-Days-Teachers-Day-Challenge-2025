import java.util.Scanner;

public class LittleElephantAndBits {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String a = sc.next();
        sc.close();
        
        int index = a.indexOf('0');  // find the first zero
        
        String result;
        if (index != -1) {
            // remove the first zero
            result = a.substring(0, index) + a.substring(index + 1);
        } else {
            // if no zero exists, remove the last digit
            result = a.substring(0, a.length() - 1);
        }
        
        System.out.println(result);
    }
}
