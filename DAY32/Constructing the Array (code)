import java.util.*;

public class RegistrationSystem {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();  // number of registration requests
        sc.nextLine(); // consume newline

        HashMap<String, Integer> database = new HashMap<>();

        for (int i = 0; i < n; i++) {
            String name = sc.nextLine();

            if (!database.containsKey(name)) {
                // If name does not exist, register it
                System.out.println("OK");
                database.put(name, 0);
            } else {
                // If name exists, find the smallest suffix number
                int count = database.get(name) + 1;
                String newName = name + count;

                // Keep incrementing until unique
                while (database.containsKey(newName)) {
                    count++;
                    newName = name + count;
                }

                System.out.println(newName);
                database.put(name, count);   // update original name's count
                database.put(newName, 0);    // register new name
            }
        }

        sc.close();
    }
}
